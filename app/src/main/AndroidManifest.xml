<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.statethefacts">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".MissedAnswersActivity"
                android:label="@string/title_activity_missed_answers"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".FactsListActivity"
                android:label="@string/title_activity_facts_list"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".ScoreCardActivity">
        </activity>
        <activity android:name=".ChartsExampleActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>